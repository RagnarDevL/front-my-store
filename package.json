{
  "name": "front-storage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd ../backend-storage && npm run start:dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "crypto-js": "^4.1.1",
    "sass": "^1.86.3",
    "sass-loader": "^13.3.3",
    "vue": "^3.5.13",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/expect": "^1.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^24.0.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vitest": "^1.5.0",
    "vue-tsc": "^2.2.4",
    "vuetify": "^3.8.1"
  }
